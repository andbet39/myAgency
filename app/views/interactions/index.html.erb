<div class="right_col" role="main">
        <div class="">

        <div class="page-title">
            <div class="title_left">
                <h3>Lista Interazioni
                </h3>
            </div>
        </div>

        <div class="clearfix"></div>

        <div class="row">
            <div class="col-md-12">
                <div class="x_panel">
                    <div class="x_content">


<table class="table table-bordered datatable" id="datatable">
  <thead>
    <tr>
      <th>Created</th>
      <th>Type</th>
      <th>Vote</th>
      <th>Interest</th>
      <th>Note</th>
      <th>Listing</th>
    </tr>
  </thead>

  <tbody>
    <% @interactions.each do |interaction| %>
      <tr>
        <td><%= interaction.created_at.to_formatted_s(:long) %></td>
        <td><%= interaction.inttype %></td>
        <td><%= interaction.vote %></td>
        <td><%= interaction.interest %></td>
        <td><%= interaction.note %></td>
        <td><%= link_to('Annuncio' , interaction.listing )%></td>
          </tr>
    <% end %>
  </tbody>
</table>
</div>
</div>
