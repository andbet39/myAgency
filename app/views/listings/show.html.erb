<h3>Interazioni</h3>
<a href="/my_interaction/new?listing_id=<%= @listing.id %>&inttype=Tel"> <button class="btn btn-success">Nuova</button></a>
  <table class="table table-condensed">
    <thead>
      <th>Id</th>
      <th>Created</th>
      <th>Nome</th>
      <th>Tipo</th>
      <th>Voto</th>
    </thead>
    <tbody>
      <% @listing.interactions.each() do | interaction | %>

      <tr>
        <td>  <%= interaction.id %></td>
        <td>  <%= interaction.created_at %></td>
        <td>  <%= interaction.name %></td>
        <td>  <%= interaction.inttype %></td>
        <td>  <%= interaction.vote %></td>
      </tr>
    <% end %>

    </tbody>
  </table>



