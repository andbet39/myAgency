<div class="right_col" role="main">
    <div class="">
        <div class="page-title">
            <div class="title_left">
                <h3>Tutti gli Annunci</h3>
            </div>
        </div>
        <div class="clearfix"></div>
        <div class="col-md-12">
            <div class="x_panel">
                <div class="x_content">
                  <div class="row">
                      <div class="col-md-12">
                          <div class="x_panel">
                              <div class="x_content">
                                      <table id="listing_table" class="table table-striped table-bordered">
                                          <thead>
                                              <tr>
                                                <th>New</th>
                                                <th>Title</th>
                                                <th>Price</th>
                                                <th>Mq</th>
                                                <th>Price/Mq</th>
                                                <th>Tel.</th>
                                                <th>Link</th>
                                                <th>Source</th>
                                                <th></th>
                                                <th></th>
                                              </tr>

                                          </thead>
                                          <tbody>
                                              <% @sr.each() do |s| %>
                                              <tr>
                                                  <% if s.is_new %>
                                                  <td ><i style="color:red" class="fa fa-exclamation red" aria-hidden="true"></i></td>
                                                  <% else %>
                                                    <td >-</td>
                                                  <% end %>
                                                  <td><%= link_to s.listing.title , s.listing %></td>
                                                  <td><%= s.listing.price %></td>
                                                  <td><%= s.listing.mt %></td>
                                                  <td><%= s.listing.pricemq %></td>
                                                  <td>
                                                      <a href="tel:<%= s.listing.tel %>"><%= s.listing.tel %></a>
                                                  </td>
                                                  <td>
                                                      <a href="<%= s.listing.link %>">Link</a>
                                                  </td>
                                                  <td><%= s.listing.origin %></td>
                                                  <td></td>
                                                  <td>
                                                      <a href="/my_interaction/new?listing_id=<%= s.listing.id %>&inttype=Tel">
                                                          <button class="btn btn-sm btn-success"><%= s.listing.interactions.length %>
                                                              <i class="fa fa-phone" aria-hidden="true"></i>
                                                          </button>
                                                      </a>
                                                  </td>
                                              </tr>
                                              <% end %>
                                          </tbody>
                                      </table>
                              </div>
                          </div>
                      </div>
                  </div>
                </div>
            </div>
        </div>
    </div>
